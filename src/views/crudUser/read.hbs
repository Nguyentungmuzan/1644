{{#> baseAdmin }}
        <div class="container" style="margin-top: 20px;">
            <form action="/" method="POST">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>Email</th>
                            <th>Role</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    {{#each users}}
                    <tbody>
                        <tr>
                            <th>{{email}}</th>
                            <th>{{role}}</th>
                            <th>
                                <a href="/updateUser/{{_id}}" class="btn border-shadow update">
                                <i class="bi bi-pencil-square"></i></a>
                                <a href="/deleteUser/{{_id}}" class="btn border-shadow update">
                                <i class="bi bi-trash"></i></a>
                            </th>
                        </tr>
                    </tbody>
                    {{/each}}
                </table>
            </form>
        </div>
{{/baseAdmin}}
